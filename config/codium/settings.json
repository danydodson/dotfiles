  "apc.imports": [
    "${userHome}/.dotfiles/.config/codium/apc/main.js",
    "${userHome}/.dotfiles/config/codium/apc/styles.css"
  ],
  // "apc.font.family": "SF Pro",
  // "apc.monospace.font.family": "SF Pro",
  // "apc.parts.font.family": {
  //   "sidebar": "SF Pro",
  //   "titlebar": "SF Pro",
  //   "activityBar": "SF Pro",
  //   "panel": "SF Pro",
  //   "tabs": "SF Pro",
  //   "statusbar": "SF Pro",
  //   "settings-body": "SF Pro",
  //   "extension-editor": "SF Pro",
  //   "monaco-menu": "SF Pro"
  // },
  "apc.electron": {
    //  "backgroundColor": "rgba(123, 123, 123, 0.5)",
    "frame": false,
    //  "transparent": true,
    //  "vibrancy": "window",
    //  "opacity": 0.96,
    //  "visualEffectState": "active",
  },
  "apc.listRow": {
    "lists": [
      "customview-tree",
      "results",
      "open-editors",
      "explorer-folders-view",
      "outline-tree", "scm-view",
      "debug-view-content",
      "debug-breakpoints",
      "tree", "explorer-folders-view",
      "results"
    ],
    "height": 23,
    "fontSize": 13.5
  },
  "apc.header": {
    "default": 24,
    "compact": 24,
    "height": 24,
    "fontSize": 10
  },
  "apc.sidebar.titlebar": {
    "fontSize": 9,
    "height": 35
  },
  "apc.statusBar": {
    "fontSize": 10,
    "height": 16
  },
  "apc.activityBar": {
    "size": 40,
    "itemSize": 26,
    "itemMargin": 16
  },
  // 
  "settings.cycle": [
    {
      "id": "lineNumbers",
      "overrideWorkspaceSettings": false,
      "values": [ { "editor.lineNumbers": "relative" }, { "editor.lineNumbers": "off" } ]
    },
    {
      "id": "tabsVisible",
      "overrideWorkspaceSettings": false,
      "values": [ { "workbench.editor.showTabs": "multiple" }, { "workbench.editor.showTabs": "none" } ]
    },
    {
      "id": "continueCompletions",
      "overrideWorkspaceSettings": false,
      "values": [ { "continue.enableTabAutocomplete": true }, { "continue.enableTabAutocomplete": false } ]
    },
    {
      "id": "codelens",
      "overrideWorkspaceSettings": false,
      "values": [ { "editor.codeLens": true }, { "editor.codeLens": false } ]
    },
    {
      "id": "color-highlight-useARGB",
      "overrideWorkspaceSettings": false,
      "values": [ { "color-highlight.useARGB": true }, { "color-highlight.useARGB": false } ]
    }
  ],
  // 
  "oneDarkPro.editorTheme": "One Dark Pro Darker",
  //

  "workbench.colorCustomizations": {
    "editorLineNumber.foreground": "#71717178",
    "tab.activeBorder": "#1f2225",
    "statusBarItem.remoteBackground": "#00000000",
    "statusBarItem.hoverBackground": "#00000000",
    "statusBarItem.hoverForeground": "#c2c5ca",
    "statusBar.border": "#202326",
    "statusBarItem.remoteForeground": "#7e8185",
    "statusBar.foreground": "#7e8185",
    "activityBar.activeBorder": "#7e8185",
    "activityBar.border": "#222528",
    "statusBar.focusBorder": "#00000000",
    "focusBorder": "#00000000"
  },
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "comment",
          "meta.var.expr storage.type",
          "keyword.control.flow",
          "keyword.control.return",
          "meta.directive.vue punctuation.separator.key-value.html",
          "meta.directive.vue entity.other.attribute-name.html",
          "tag.decorator.js entity.name.tag.js",
          "tag.decorator.js punctuation.definition.tag.js",
          "storage.modifier"
        ],
        "settings": {
          "fontStyle": ""
        }
      },
      {
        "scope": [ "comment keyword.codetag.notation", "comment.block.documentation keyword", "comment.block.documentation storage.type.class" ],
        "settings": { "foreground": "#bb9af7" }
      },
      {
        "scope": [ "comment.block.documentation entity.name.type.instance" ],
        "settings": { "foreground": "#73daca", "fontStyle": "italic" }
      },
      {
        "scope": [ "comment.block.documentation entity.name.type punctuation.definition.bracket" ],
        "settings": { "foreground": "#bb9af7" }
      },
      {
        "scope": [ "comment.block.documentation variable" ],
        "settings": { "foreground": "#e0af68", "fontStyle": "italic" }
      }
    ]
  },
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb",
    "*.sqlite": "${capture}.${extname}-*",
    "*.db": "${capture}.${extname}-*",
    "*.sqlite3": "${capture}.${extname}-*",
    "*.db3": "${capture}.${extname}-*",
    "*.sdb": "${capture}.${extname}-*",
    "*.s3db": "${capture}.${extname}-*"
  },
  "liveServer.settings.donotVerifyTags": true,
  "[shellscript]": {
    "editor.defaultFormatter": "mads-hartmann.bash-ide-vscode"
  },
  "deepl.formality": "default",
  "deepl.tagHandling": "off",
  "deepl.splitSentences": "1",
  "deepl.translationMode": "Replace",
  "sherlock.userId": "971f9f90-d365-48fb-b236-80e7b4f3d52c",
  "Lua.codeLens.enable": true,
  "javascript.referencesCodeLens.enabled": true,
  "typescript.referencesCodeLens.enabled": true,
  // "diffEditor.codeLens": true,
  "workbench.editor.tabSizing": "shrink",
}
