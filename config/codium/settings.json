  "applescript.osacompile.startupScreen": true,
  //
  "notebook.output.fontSize": 12,
  // 
  "python.createEnvironment.contentButton": "show",
  // 
  "tldr-panel.defaultLanguage": "en",
  "tldr-panel.defaultPlatform": "osx",
  //
  "prettier.tabWidth": 2,
  "prettier.semi": true,
  // 
  "1password.debug.enabled": true,
  //
  "color-highlight.markRuler": false,
  "color-highlight.markerType": "foreground",
  "color-highlight.useARGB": true,
  "color-highlight.matchHslWithNoFunction": true,
  "color-highlight.matchRgbWithNoFunction": true,
