  "Lua.diagnostics.disable": [
    "cast-local-type",
    "duplicate-set-field",
    "inject-field",
    "lowercase-global",
    "param-type-mismatch",
    "need-check-nil",
    "redundant-parameter",
    "unbalanced-assignments",
    "undefined-field",
    "undefined-global",
    "undefined-doc-name",
    "deprecated"
