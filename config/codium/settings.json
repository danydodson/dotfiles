  "svg.preview.mode": "svg",
  "Lua.diagnostics.globals": [
    "Command",
    "cx",
    "err",
    "Err",
    "fs",
    "PREVIEW",
    "rt",
    "sbar",
    "th",
    "ui",
    "units",
    "Url",
    "ya",
    "lfs"
