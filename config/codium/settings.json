{
  "update.mode": "manual",
  //
  "security.workspace.trust.enabled": false,
  //
  "telemetry.telemetryLevel": "off",
  // 
  "redhat.telemetry.enabled": false,
  // 
  "continue.enableTabAutocomplete": false,
  // "continue.remoteConfigSyncPeriod": 60,
  "continue.telemetryEnabled": false,
  "continue.enableConsole": false,
  "continue.enableQuickActions": false,
  "continue.pauseCodebaseIndexOnStart": false,
  "continue.showInlineTip": true,
  // "continue.disableQuickFix": false,
  // 
  "code-runner.enableAppInsights": false,
  // 
  "diffEditor.hideUnchangedRegions.enabled": true,
  // 
  "1password.debug.enabled": true,
  //
  "m3u8.chrome.profileDirectory": "Default",
  "m3u8.chrome.executablePath": "/Applications/Brave Browser.app",
  // 
  "github.gitProtocol": "ssh",
  // 
  "svg.preview.mode": "svg",
  // 
  "git.confirmSync": false,
  "git.enableStatusBarSync": false,
  "git.enableSmartCommit": true,
  "git.openRepositoryInParentFolders": "never",
  "git.defaultCloneDirectory": "~/Developer/repos",
  "git.decorations.enabled": true,
  "git.autofetch": true,
  "git.branchRandomName.enable": true,
  "git.countBadge": "off",
  "git.enableCommitSigning": true,
  "git.scanRepositories": [
    "~/Developer/plugins",
    "~/Developer/repos",
    "~/Developer/served",
    "~/Developer/temp",
    "/opt/homebrew"
  ],
  //
  "scm.countBadge": "off",
  "scm.repositories.visible": 0,
  "scm.diffDecorations": "gutter",
  "scm.diffDecorationsGutterWidth": 1,
  "scm.diffDecorationsGutterVisibility": "hover",
  // 
  "breadcrumbs.enabled": false,
  //
  "autoOpenPreviewPanel.languages": "markdown,asciidoc",
  //
  "npm-intellisense.importLinebreak": "",
  "npm-intellisense.packageSubfoldersIntellisense": true,
  "npm-intellisense.scanDevDependencies": true,
  //
  "liveServer.settings.donotShowInfoMsg": true,
  //
  "notebook.output.fontSize": 12,
  // 
  "prettier.tabWidth": 2,
  "prettier.semi": true,
  // 
  // "color-highlight.sass.includePaths": [],
  "color-highlight.markRuler": false,
  "color-highlight.markerType": "foreground",
  "color-highlight.useARGB": true,
  // "color-highlight.useARGB": true,
  // "color-highlight.useARGB": false,
  "color-highlight.matchHslWithNoFunction": true,
  "color-highlight.matchRgbWithNoFunction": true,
  // 
  "explorer.incrementalNaming": "smart",
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "explorer.compactFolders": false,
  "explorer.decorations.badges": false,
  // 
  "window.customTitleBarVisibility": "never",
  "window.commandCenter": false,
  "window.nativeFullScreen": false,
  "window.titleBarStyle": "native",
  "window.confirmSaveUntitledWorkspace": false,
  // 
  "workbench.startupEditor": "none",
  "workbench.layoutControl.enabled": false,
  "workbench.editor.empty.hint": "hidden",
  "workbench.editor.showTabs": "multiple",
  "workbench.panel.showLabels": false,
  "workbench.list.fastScrollSensitivity": 5,
  "workbench.tree.enableStickyScroll": false,
  "workbench.tree.indent": 14,
  "workbench.editor.revealIfOpen": true,
  "workbench.settings.editor": "json",
  "workbench.editor.decorations.badges": false,
  "workbench.colorTheme": "One Dark Pro Darker",
  //
  "editor.lightbulb.enabled": "onCode",
  "editor.largeFileOptimizations": false,
  "editor.overviewRulerBorder": false,
  "editor.hideCursorInOverviewRuler": false,
  "editor.codeLens": false,
  "editor.autoClosingQuotes": "never",
  "editor.autoClosingBrackets": "never",
  "editor.fontSize": 14.3,
  "editor.lineHeight": 22,
  "editor.tabSize": 2,
  "editor.folding": false,
  "editor.padding.top": 15,
  "editor.padding.bottom": 15,
  "editor.cursorStyle": "line",
  "editor.cursorBlinking": "solid",
  "editor.renderLineHighlight": "none",
  // "editor.glyphMargin": false,
  "editor.lineNumbers": "relative",
  "editor.lineDecorationsWidth": 16,
  "editor.stickyScroll.enabled": false,
  "editor.fontWeight": 400,
  "editor.fontFamily": "FiraCode Nerd Font",
  "editor.fontLigatures": true,

  // "editor.colorDecoratorsLimit": 500,
  "editor.colorDecoratorsActivatedOn": "click",
  "editor.defaultColorDecorators": true,
  "editor.minimap.enabled": false,
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.verticalScrollbarSize": 8,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "editor.unicodeHighlight.invisibleCharacters": false,
  "editor.unicodeHighlight.nonBasicASCII": false,
  //
  "terminal.integrated.fontSize": 15,
  "terminal.integrated.lineHeight": 1.1,
  "terminal.integrated.fontWeightBold": "normal",
  "terminal.integrated.fontFamily": "FiraMono Nerd Font",
  "terminal.integrated.drawBoldTextInBrightColors": false,
  "terminal.integrated.inheritEnv": false,
  "terminal.integrated.stickyScroll.enabled": false,
  "terminal.integrated.enableImages": true,
  "terminal.integrated.scrollback": 100000,
  "terminal.integrated.shellIntegration.decorationsEnabled": "never",
  "terminal.integrated.allowedLinkSchemes": [
    "file",
    "http",
    "https",
    "mailto",
    "vscode",
    "vscode-insiders",
    "docker-desktop"
  ],
  //
  "extensions.autoCheckUpdates": false,
  "extensions.ignoreRecommendations": true,
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  //
  "diffEditor.wordWrap": "off",
  "diffEditor.renderGutterMenu": true,
  //
  "debug.toolBarLocation": "docked",
  "debug.console.wordWrap": false,
  "debug.javascript.codelens.npmScripts": "never",
  //
  "shellformat.path": "/opt/homebrew/bin/shfmt",
  //
  "files.associations": {
    "gitlocal": "properties",
    "gitalias": "",
    "gitsecret": "properties",
    "*tmpl": "html",
    "dircolors*": "dircolors"
  },
  // 
  "[css]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "[dockercompose]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[dockerfile]": {
    // "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "gopls": { "ui.semanticTokens": true },
  "go.toolsManagement.autoUpdate": true,
  "[go]": {
    // "editor.semanticHighlighting.enabled": false
  },
  "[handlebars]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "html.format.wrapLineLength": 9999,
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "javascript.format.semicolons": "insert",
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "json.format.keepLines": true,
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "stylua.searchParentDirectories": true,
  "[lua]": {
    "editor.defaultFormatter": "JohnnyMorganz.stylua"
  },
  "[luau]": {
    "editor.defaultFormatter": "JohnnyMorganz.stylua"
  },
  "markdown.preview.doubleClickToSwitchToEditor": false,
  "[markdown]": {
    "editor.wordWrap": "off",
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.autopep8"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[svg]": {
    "editor.defaultFormatter": "jock.svg",
    "editor.wordWrap": "off"
  },
  "evenBetterToml.formatter.allowedBlankLines": 2,
  "evenBetterToml.formatter.arrayAutoCollapse": false,
  "evenBetterToml.formatter.columnWidth": 380,
  "evenBetterToml.formatter.trailingNewline": true,
  "evenBetterToml.formatter.compactArrays": false,
  "[toml]": {
    "editor.defaultFormatter": "tamasfe.even-better-toml"
  },
  "typescript.format.semicolons": "insert",
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "vue.server.hybridMode": false,
  "[vue]": {
    "editor.defaultFormatter": "Vue.volar"
  },
  "[xml]": {
    "breadcrumbs.showArrays": true
  },
  "yaml.schemas": {
    "/Applications/codium-portable-data/extensions/continue.continue-1.0.19-darwin-arm64/config-yaml-schema.json": [
      ".continue/**/*.yaml"
    ]
  },
  "[yaml]": {
    "editor.wordWrap": "off",
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "zig.zls.enabled": "on",
  //
